{
    "name": "emtiazzahid/laravel-rat",
    "description": "A package to bulk image resize in a directory or model",
    "keywords": [
        "emtiazzahid",
        "laravel-rat"
    ],
    "homepage": "https://github.com/emtiazzahid/laravel-rat",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "emtiazzahid",
            "email": "emtiazzahid@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Emtiazzahid\\LaravelRat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emtiazzahid\\LaravelRat\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emtiazzahid\\LaravelRat\\LaravelRatServiceProvider"
            ],
            "aliases": {
                "LaravelRat": "Emtiazzahid\\LaravelRat\\LaravelRatFacade"
            }
        }
    }
}
